    "connect-redis": "^6.1.3",
    "express-session": "^1.17.3",
    "mongoose": "^5.12.7",
    "nestjs-redis": "^1.3.3",
    "nestjs-session": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7",
    "socket.io": "^3.1.2",
    "transliteration": "^2.2.0",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@nestjs/typeorm": "^8.0.4",
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.11",
